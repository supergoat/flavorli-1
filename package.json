{
  "name": "flavorli",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "^2.1.8",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "node node_modules/eslint/bin/eslint.js --config node_modules/eslint-config-react-app/index.js src --ext .js,.jsx,.ts,.tsx",
    "format": "yarn prettier --write",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "yarn lint && yarn test && yarn format && yarn prettier --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn validate"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.11",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "1.16.4",
    "typescript": "^3.3.3333"
  }
}
